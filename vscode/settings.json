{
  "markdown.extension.preview.autoShowPreviewToSide": false,
  "editor.minimap.enabled": false,
  "editor.formatOnPaste": false,
  "editor.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontWeight": "200",
  "files.exclude": {
    "**/__pycache__": true,
    "**/_build": true,
    "**/_deploy": true,
    "**/_site": true,
    "**/.bundle": true,
    "**/.coverage": true,
    "**/.DS_Store": true,
    "**/.expo": true,
    "**/.git": true,
    "**/.githooks": true,
    "**/.hg": true,
    "**/.pygments-cache": true,
    "**/.sass-cache": true,
    "**/.svn": true,
    "**/.tox": true,
    "**/.vagrant": true,
    "**/.venv": true,
    "**/.virtualenv": true,
    "**/*.a": true,
    "**/*.class": true,
    "**/*.db": true,
    "**/*.dll": true,
    "**/*.dylib": true,
    "**/*.egg-info": true,
    "**/*.exe": true,
    "**/*.idb": true,
    "**/*.lib": true,
    "**/*.ncb": true,
    "**/*.o": true,
    "**/*.obj": true,
    "**/*.pdb": true,
    "**/*.psd": true,
    "**/*.pyc": true,
    "**/*.pyo": true,
    "**/*.sdf": true,
    "**/*.so": true,
    "**/*.sublime-workspace": true,
    "**/*.suo": true,
    "**/backstop_data": true,
    "**/build": true,
    "**/coverage": true,
    "**/css_regression_data": true,
    "**/CVS": true,
    "**/db.sqlite3": true,
    "**/env": true,
    "**/es5": true,
    "**/npm-debug.log*": true,
    "**/npm-shrinkwrap.json": true,
    "**/static/**/*.css": false,
    "**/static/**/*.js": false,
    "**/static/wagtailadmin": true,
    "**/tmp": true,
    "**/var": true,
    "**/venv": true,
    "**/www": true
  },
  "window.reopenFolders": "all",
  "window.autoDetectColorScheme": true,
  "terminal.external.osxExec": "iTerm2.app",
  "workbench.iconTheme": "material-icon-theme",
  "editor.tabSize": 2,
  "git.autofetch": true,
  "css.validate": true,
  "scss.validate": true,
  "jsAutolint.defaultLinters": ["eslint", "standard"],
  "jsAutolint.enable": true,
  "typescript.check.npmIsInstalled": false,
  "editor.fontSize": 15,
  "prettier.singleQuote": true,
  "javascript.format.enable": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "markdown.preview.linkify": false,
  "markdown.preview.markEditorSelection": false,
  "eslint.packageManager": "npm",
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "cSpell.enabledLanguageIds": [
    "css",
    "git-commit",
    "html",
    "javascript",
    "json",
    "scss",
    "text"
  ],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[git-commit]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 72
  },
  "workbench.preferredDarkColorTheme": "Night Owl",
  "workbench.preferredLightColorTheme": "Night Owl Light",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "files.associations": {
    ".env.development.local": "shellscript"
  },
  "files.insertFinalNewline": true,
  "workbench.colorTheme": "Night Owl Light",
  "extensions.ignoreRecommendations": true,
  "rewrap.wrappingColumn": 72,
  "gitlens.rebaseEditor.ordering": "asc"
}
